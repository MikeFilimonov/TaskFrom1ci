	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.ContactPerson</Field>
				<Field>List.Counterparty</Field>
				<Field>List.Ref</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	DocumentQuote.Ref,
	DocumentQuote.DeletionMark,
	DocumentQuote.Number,
	DocumentQuote.Date,
	DocumentQuote.Posted,
	DocumentQuote.Company,
	DocumentQuote.Comment,
	DocumentQuote.Counterparty,
	DocumentQuote.Counterparty.ContactPerson AS ContactPerson,
	DocumentQuote.Contract,
	DocumentQuote.CashAssetsType,
	DocumentQuote.BankAccount,
	DocumentQuote.DocumentCurrency,
	DocumentQuote.Multiplicity,
	DocumentQuote.ExchangeRate,
	DocumentQuote.AmountIncludesVAT,
	DocumentQuote.PriceKind,
	DocumentQuote.DiscountMarkupKind,
	DocumentQuote.Department,
	DocumentQuote.DocumentAmount,
	DocumentQuote.PettyCash,
	DocumentQuote.Responsible,
	DocumentQuote.PointInTime,
	DocumentQuote.Author,
	CASE
		WHEN DocumentQuote.Posted
				AND DocumentQuote.DocumentAmount &gt; 0
			THEN CAST((ISNULL(PaymentEvent.PaymentAmount, 0) + ISNULL(PaymentEvent.AdvanceAmount, 0)) * 100 / DocumentQuote.DocumentAmount AS NUMBER(15, 2))
		ELSE 0
	END AS PaymentPercentage,
	ISNULL(PaymentEvent.PaymentAmount, 0) + ISNULL(PaymentEvent.AdvanceAmount, 0) AS PaymentAmountTurnover,
	CASE WHEN DocumentQuote.Posted THEN ISNULL(QuotationStatuses.Status, VALUE(Enum.QuotationStatuses.Sent)) ELSE VALUE(Enum.QuotationStatuses.Draft) END AS Status
	FROM
	Document.Quote AS DocumentQuote
		LEFT JOIN InformationRegister.OrderPayments AS PaymentEvent
		ON DocumentQuote.Ref = PaymentEvent.Quote
		LEFT JOIN InformationRegister.QuotationStatuses AS QuotationStatuses
		ON QuotationStatuses.Document = DocumentQuote.Ref
		{LEFT JOIN InformationRegister.OrdersPaymentSchedule AS PaymentSchedule
		ON DocumentQuote.Ref = PaymentSchedule.Quote
				AND (PaymentSchedule.Period &lt;= &amp;CurrentDateSession)}</QueryText>
				<MainTable>Document.Quote</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
