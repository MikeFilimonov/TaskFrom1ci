			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	DocumentSalesOrder.Ref,
	DocumentSalesOrder.DataVersion,
	DocumentSalesOrder.DeletionMark,
	DocumentSalesOrder.Number,
	DocumentSalesOrder.Date,
	DocumentSalesOrder.Posted,
	DocumentSalesOrder.Author,
	DocumentSalesOrder.BankAccount,
	DocumentSalesOrder.DocumentCurrency,
	DocumentSalesOrder.OperationKind,
	DocumentSalesOrder.WorkKind,
	DocumentSalesOrder.DiscountMarkupKind,
	DocumentSalesOrder.PriceKind,
	DocumentSalesOrder.ShipmentDate,
	DocumentSalesOrder.Contract,
	DocumentSalesOrder.Closed,
	DocumentSalesOrder.SetPaymentTerms,
	DocumentSalesOrder.PettyCash,
	DocumentSalesOrder.Comment,
	DocumentSalesOrder.Counterparty,
	DocumentSalesOrder.Multiplicity,
	DocumentSalesOrder.ExchangeRate,
	DocumentSalesOrder.IncludeVATInPrice,
	DocumentSalesOrder.Company,
	DocumentSalesOrder.Responsible,
	DocumentSalesOrder.Project,
	DocumentSalesOrder.Event,
	DocumentSalesOrder.OrderState,
	CASE
		WHEN (DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.Open)
				OR DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.InProcess))
				AND Not DocumentSalesOrder.Closed
			THEN "In process"
		ELSE CASE
				WHEN DocumentSalesOrder.OrderState.OrderStatus = VALUE(Enum.OrderStatuses.Completed)
					THEN "Completed"
				ELSE "Canceled"
			END
	END AS OrderStatus,
	DocumentSalesOrder.Start,
	DocumentSalesOrder.SalesStructuralUnit,
	DocumentSalesOrder.StructuralUnitReserve,
	DocumentSalesOrder.VATTaxation,
	DocumentSalesOrder.AmountIncludesVAT,
	DocumentSalesOrder.DocumentAmount,
	DocumentSalesOrder.CashAssetsType,
	DocumentSalesOrder.Finish,
	DocumentSalesOrder.Cell,
	DocumentSalesOrder.WorkKindPosition,
	DocumentSalesOrder.ShipmentDatePosition,
	DocumentSalesOrder.UseProducts,
	DocumentSalesOrder.UseConsumerMaterials,
	DocumentSalesOrder.UseMaterials,
	DocumentSalesOrder.UsePerformerSalaries,
	DocumentSalesOrder.ProductsList,
	DocumentSalesOrder.ResourcesList,
	DocumentSalesOrder.PointInTime
FROM
		Document.SalesOrder AS DocumentSalesOrder
			INNER JOIN Catalog.SalesOrderStatuses AS SalesOrderStatuses
			ON DocumentSalesOrder.OrderState = SalesOrderStatuses.Ref
				AND (SalesOrderStatuses.OrderStatus &lt;&gt; VALUE(Enum.OrderStatuses.Open))</QueryText>
				<MainTable>Document.SalesOrder</MainTable>
