			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	DocumentSupplierInvoice.Ref AS Ref,
	DocumentSupplierInvoice.DeletionMark AS DeletionMark,
	DocumentSupplierInvoice.Number AS Number,
	DocumentSupplierInvoice.Date AS Date,
	DocumentSupplierInvoice.Posted AS Posted,
	DocumentSupplierInvoice.Company AS Company,
	DocumentSupplierInvoice.Counterparty AS Counterparty,
	DocumentSupplierInvoice.Contract AS Contract,
	DocumentSupplierInvoice.BasisDocument AS BasisDocument,
	DocumentSupplierInvoice.Order AS Order,
	DocumentSupplierInvoice.StructuralUnit AS StructuralUnit,
	DocumentSupplierInvoice.Cell AS Cell,
	DocumentSupplierInvoice.IncludeExpensesInCostPrice AS IncludeExpensesInCostPrice,
	DocumentSupplierInvoice.DocumentCurrency AS DocumentCurrency,
	DocumentSupplierInvoice.VATTaxation AS VATTaxation,
	DocumentSupplierInvoice.AmountIncludesVAT AS AmountIncludesVAT,
	DocumentSupplierInvoice.IncludeVATInPrice AS IncludeVATInPrice,
	DocumentSupplierInvoice.ExchangeRate AS ExchangeRate,
	DocumentSupplierInvoice.Multiplicity AS Multiplicity,
	DocumentSupplierInvoice.DocumentAmount AS DocumentAmount,
	DocumentSupplierInvoice.SupplierPriceTypes AS SupplierPriceTypes,
	DocumentSupplierInvoice.RegisterVendorPrices AS RegisterVendorPrices,
	DocumentSupplierInvoice.IncomingDocumentNumber AS IncomingDocumentNumber,
	DocumentSupplierInvoice.IncomingDocumentDate AS IncomingDocumentDate,
	DocumentSupplierInvoice.Comment AS Comment,
	DocumentSupplierInvoice.Department AS Department,
	DocumentSupplierInvoice.Responsible AS Responsible,
	DocumentSupplierInvoice.Author AS Author,
	DocumentSupplierInvoice.PurchaseOrderPosition AS PurchaseOrderPosition,
	DocumentSupplierInvoice.DiscountCard AS DiscountCard,
	DocumentSupplierInvoice.SalesSlip AS SalesSlip,
	ISNULL(InvoicesPaymentStatuses.Status, VALUE(Enum.InvoicesPaymentStatuses.Unpaid)) AS PaymentStatus
FROM
	Document.SupplierInvoice AS DocumentSupplierInvoice
		LEFT JOIN InformationRegister.InvoicesPaymentStatuses AS InvoicesPaymentStatuses
		ON (InvoicesPaymentStatuses.Document = DocumentSupplierInvoice.Ref)</QueryText>
