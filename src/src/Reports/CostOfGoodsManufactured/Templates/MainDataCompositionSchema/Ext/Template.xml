				<field>ProductCharacteristic</field>
			</field>
			<field xsi:type="DataSetFieldField">
				<dataPath>UnOfMeas</dataPath>
				<field>UnOfMeas</field>
			</field>
			<field xsi:type="DataSetFieldField">
				<dataPath>UnOfMeasProducts</dataPath>
				<field>UnOfMeasProducts</field>
			</field>
			<field xsi:type="DataSetFieldField">
				<dataPath>ProductionSubdepartment</dataPath>
				<field>ProductionSubdepartment</field>
				<title xsi:type="v8:LocalStringType">
					<v8:item>
						<v8:lang>en</v8:lang>
						<v8:content>Production subdepartment</v8:content>
					</v8:item>
				</title>
			</field>
			<field xsi:type="DataSetFieldField">
				<dataPath>ProductsCorr</dataPath>
				<field>ProductsCorr</field>
			</field>
			<dataSource>DataSource1</dataSource>
			<query>SELECT ALLOWED
	Inventory.ProductsCorr AS ProductsCorr,
	Inventory.CharacteristicCorr AS ProductCharacteristic,
	Inventory.BatchCorr AS BatchProducts,
	Inventory.CorrGLAccount AS ProductsGLAccount,
	Inventory.CustomerCorrOrder AS SalesOrderProducts,
	Inventory.SpecificationCorr AS ProductionSpecification,
	Inventory.StructuralUnitCorr AS ProductionSubdepartment,
	Inventory.Company AS CompanyProducts,
	CASE
		WHEN Inventory.Products = VALUE(Catalog.Products.EmptyRef)
			THEN "Intangible costs"
		ELSE Inventory.Products
	END AS Products,
	Inventory.Characteristic AS Characteristic,
	Inventory.Batch AS Batch,
	Inventory.GLAccount AS GLAccount,
	Inventory.Amount AS Amount,
	Inventory.Quantity AS Quantity,
	0 AS ProductsQuantity,
	Inventory.Specification AS Specification,
	Inventory.Products.MeasurementUnit AS UnOfMeas,
	Inventory.ProductsCorr.MeasurementUnit AS UnOfMeasProducts
FROM
	AccumulationRegister.Inventory AS Inventory
WHERE
	Inventory.RecordType = VALUE(AccumulationRecordType.Expense)
	AND Inventory.ProductionExpenses
	AND CASE
			WHEN &amp;EndOfPeriod = DATETIME(1, 1, 1)
				THEN Inventory.Period &gt;= &amp;BeginOfPeriod
			ELSE Inventory.Period BETWEEN &amp;BeginOfPeriod AND &amp;EndOfPeriod
		END</query>
		</item>
