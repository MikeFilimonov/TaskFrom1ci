		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	Employees.Code AS TabNumber,
	Employees.Ref AS Employee,
	Employees.Ind AS Ind,
	Employees.EmploymentContractType AS EmploymentContractType,
	Employees.Ind.Birthdate AS BirthDate,
	Employees.Ind.Gender AS Gender,
	TabPostalAddress.Presentation AS IndividualPostalAddress,
	TabActualAddress.Presentation AS IndividualActualAddress,
	TabAnother.Presentation AS OtherContactInformation,
	TabPhone.Presentation AS Phone,
	EmployeesSliceLast.Recorder AS PersonnelRecordsRecorder,
	EmployeesSliceLast.Company AS Company,
	EmployeesSliceLast.StructuralUnit AS Department,
	EmployeesSliceLast.Position AS Position,
	EmployeesSliceLast.OccupiedRates AS OccupiedRates,
	EmployeesSliceLast.WorkSchedule AS WorkSchedule,
	EmployeesSliceLast.Period AS PeriodStaff,
	CASE
		WHEN EmployeesSliceLast.StructuralUnit IS NULL
				OR EmployeesSliceLast.StructuralUnit = VALUE(Catalog.BusinessUnits.EmptyRef)
			THEN FALSE
		ELSE TRUE
	END AS Works,
	ChangeHistoryOfIndividualNamesSliceLast.Surname AS Surname,
	ChangeHistoryOfIndividualNamesSliceLast.Name AS Name,
	ChangeHistoryOfIndividualNamesSliceLast.Patronymic AS Patronymic,
	LegalDocuments.DocumentKind AS DocumentKind,
	LegalDocuments.Number AS Number,
	LegalDocuments.Authority AS WhoIssued,
	LegalDocuments.IssueDate AS IssueDate
FROM
	Catalog.Employees AS Employees
		LEFT JOIN Catalog.Individuals.ContactInformation AS TabActualAddress
		ON Employees.Ind.Ref = TabActualAddress.Ref
			AND (TabActualAddress.Kind = VALUE(Catalog.ContactInformationTypes.IndividualActualAddress))
		LEFT JOIN Catalog.Individuals.ContactInformation AS TabPostalAddress
		ON Employees.Ind.Ref = TabPostalAddress.Ref
			AND (TabPostalAddress.Kind = VALUE(Catalog.ContactInformationTypes.IndividualPostalAddress))
		LEFT JOIN Catalog.Individuals.ContactInformation AS TabPhone
		ON Employees.Ind.Ref = TabPhone.Ref
			AND (TabPhone.Kind = VALUE(Catalog.ContactInformationTypes.IndividualPhone))
		LEFT JOIN Catalog.Individuals.ContactInformation AS TabAnother
		ON Employees.Ind.Ref = TabAnother.Ref
			AND (TabAnother.Kind = VALUE(Catalog.ContactInformationTypes.IndividualOtherInformation))
		LEFT JOIN InformationRegister.Employees.SliceLast AS EmployeesSliceLast
		ON Employees.Ref = EmployeesSliceLast.Employee
		LEFT JOIN InformationRegister.ChangeHistoryOfIndividualNames.SliceLast AS ChangeHistoryOfIndividualNamesSliceLast
		ON Employees.Ind = ChangeHistoryOfIndividualNamesSliceLast.Ind
		LEFT JOIN Catalog.LegalDocuments AS LegalDocuments
		ON Employees.Ind = LegalDocuments.Owner</query>
