		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CASE
		WHEN SalesOrdersBalanceAndTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE SalesOrdersBalanceAndTurnovers.Recorder
	END AS Recorder,
	SalesOrdersBalanceAndTurnovers.SecondPeriod,
	SalesOrdersBalanceAndTurnovers.DayPeriod,
	SalesOrdersBalanceAndTurnovers.MonthPeriod,
	SalesOrdersBalanceAndTurnovers.Company,
	SalesOrdersBalanceAndTurnovers.SalesOrder,
	SalesOrdersBalanceAndTurnovers.SalesOrder.Counterparty AS Counterparty,
	SalesOrdersBalanceAndTurnovers.SalesOrder.Contract AS Contract,
	SalesOrdersBalanceAndTurnovers.SalesOrder.Project AS Project,
	SalesOrdersBalanceAndTurnovers.Products,
	SalesOrdersBalanceAndTurnovers.Characteristic,
	SalesOrdersBalanceAndTurnovers.QuantityReceipt AS Ordered,
	SalesOrdersBalanceAndTurnovers.QuantityExpense AS Shipped,
	ReservationOrdersBalance.Reserved AS ReservedAtWarehouse,
	BackordersBalances.OrderedToSuppliers AS OrderedToSuppliers,
	BackordersBalances.InProduction AS InProduction,
	SalesOrdersBalanceAndTurnovers.QuantityClosingBalance AS RemainingToShip,
	CASE
		WHEN SalesOrdersBalanceAndTurnovers.Products.ProductsType = VALUE(Enum.ProductsTypes.InventoryItem)
			THEN SalesOrdersBalanceAndTurnovers.QuantityClosingBalance - ISNULL(BackordersBalances.OrderedToSuppliers, 0) - ISNULL(ReservationOrdersBalance.Reserved, 0) - ISNULL(BackordersBalances.InProduction, 0)
		ELSE 0
	END AS LeftToProvide,
	SalesOrdersBalanceAndTurnovers.Products.MeasurementUnit,
	SalesOrdersBalanceAndTurnovers.MinutePeriod,
	SalesOrdersBalanceAndTurnovers.HourPeriod,
	SalesOrdersBalanceAndTurnovers.WeekPeriod,
	SalesOrdersBalanceAndTurnovers.TenDaysPeriod,
	SalesOrdersBalanceAndTurnovers.QuarterPeriod,
	SalesOrdersBalanceAndTurnovers.HalfYearPeriod,
	SalesOrdersBalanceAndTurnovers.YearPeriod,
	SalesOrdersBalanceAndTurnovers.SalesOrder.Closed AS Closed,
	SalesOrdersBalanceAndTurnovers.SalesOrder.OrderState AS OrderState,
	SalesOrdersBalanceAndTurnovers.SalesOrder.OrderState.OrderStatus AS OrderStatus
	FROM
	AccumulationRegister.SalesOrders.BalanceAndTurnovers(, , Auto, RegisterRecords, ) AS SalesOrdersBalanceAndTurnovers
		LEFT JOIN (SELECT
			PlacementBalances.Products AS Products,
			PlacementBalances.Characteristic AS Characteristic,
			PlacementBalances.Company AS Company,
			PlacementBalances.SalesOrder AS SalesOrder,
			SUM(CASE
					WHEN PlacementBalances.SupplySource REFS Document.PurchaseOrder
						THEN PlacementBalances.QuantityBalance
					ELSE 0
				END) AS OrderedToSuppliers,
			SUM(CASE
					WHEN PlacementBalances.SupplySource REFS Document.ProductionOrder
						THEN PlacementBalances.QuantityBalance
					ELSE 0
				END) AS InProduction
		FROM
			AccumulationRegister.Backorders.Balance AS PlacementBalances
		
		GROUP BY
			PlacementBalances.Products,
			PlacementBalances.Characteristic,
			PlacementBalances.Company,
			PlacementBalances.SalesOrder) AS BackordersBalances
		ON SalesOrdersBalanceAndTurnovers.Company = BackordersBalances.Company
			AND SalesOrdersBalanceAndTurnovers.SalesOrder = BackordersBalances.SalesOrder
			AND SalesOrdersBalanceAndTurnovers.Products = BackordersBalances.Products
			AND SalesOrdersBalanceAndTurnovers.Characteristic = BackordersBalances.Characteristic
		LEFT JOIN (SELECT
			InventoryBalances.Products AS Products,
			InventoryBalances.Characteristic AS Characteristic,
			InventoryBalances.Company AS Company,
			InventoryBalances.SalesOrder AS SalesOrder,
			InventoryBalances.QuantityBalance AS Reserved
		FROM
			AccumulationRegister.Inventory.Balance(
					,
					SalesOrder &lt;&gt; UNDEFINED
						AND StructuralUnit REFS Catalog.BusinessUnits) AS InventoryBalances) AS ReservationOrdersBalance
		ON SalesOrdersBalanceAndTurnovers.Company = ReservationOrdersBalance.Company
			AND SalesOrdersBalanceAndTurnovers.SalesOrder = ReservationOrdersBalance.SalesOrder
			AND SalesOrdersBalanceAndTurnovers.Products = ReservationOrdersBalance.Products
			AND SalesOrdersBalanceAndTurnovers.Characteristic = ReservationOrdersBalance.Characteristic
	WHERE
	CASE
			WHEN &amp;FilterByOrders = "Opened"
				THEN Not SalesOrdersBalanceAndTurnovers.SalesOrder.Closed
			WHEN &amp;FilterByOrders = "NotShipped"
				THEN SalesOrdersBalanceAndTurnovers.QuantityReceipt &gt; SalesOrdersBalanceAndTurnovers.QuantityExpense
			WHEN &amp;FilterByOrders = "NotAllocated"
				THEN SalesOrdersBalanceAndTurnovers.QuantityReceipt &gt; SalesOrdersBalanceAndTurnovers.QuantityExpense
						AND ISNULL(SalesOrdersBalanceAndTurnovers.QuantityClosingBalance, 0) - ISNULL(BackordersBalances.OrderedToSuppliers, 0) - ISNULL(ReservationOrdersBalance.Reserved, 0) - ISNULL(BackordersBalances.InProduction, 0) &gt; 0
			ELSE TRUE
			END</query>
	</dataSet>
