		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CASE
		WHEN StockReceivedFromThirdPartiesBalancesAndTurnovers.Recorder = UNDEFINED
			THEN NULL
		ELSE StockReceivedFromThirdPartiesBalancesAndTurnovers.Recorder
	END AS Recorder,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.SecondPeriod AS SecondPeriod,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.MinutePeriod AS MinutePeriod,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.HourPeriod AS HourPeriod,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.DayPeriod AS DayPeriod,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.WeekPeriod AS WeekPeriod,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.TenDaysPeriod AS TenDaysPeriod,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.MonthPeriod AS MonthPeriod,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.QuarterPeriod AS QuarterPeriod,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.HalfYearPeriod AS HalfYearPeriod,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.YearPeriod AS YearPeriod,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.Company AS Company,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.Products AS Products,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.Characteristic AS Characteristic,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.Batch AS Batch,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.Counterparty AS Counterparty,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.Contract AS Contract,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.Order AS Order,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.QuantityOpeningBalance AS QuantityOpeningBalance,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.QuantityClosingBalance AS QuantityClosingBalance,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.QuantityTurnover AS QuantityTurnover,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.QuantityReceipt AS QuantityReceipt,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.QuantityExpense AS QuantityExpense,
	StockReceivedFromThirdPartiesBalancesAndTurnovers.Products.MeasurementUnit AS ProductsMeasurementUnit
FROM
	AccumulationRegister.StockReceivedFromThirdParties.BalanceAndTurnovers(, , Auto, RegisterRecords, ) AS StockReceivedFromThirdPartiesBalancesAndTurnovers</query>
	</dataSet>
