		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	CASE
		WHEN InventoryTurnover.Recorder = UNDEFINED
			THEN NULL
		ELSE InventoryTurnover.Recorder
	END AS Recorder,
	InventoryTurnover.SecondPeriod AS SecondPeriod,
	InventoryTurnover.MinutePeriod AS MinutePeriod,
	InventoryTurnover.HourPeriod AS HourPeriod,
	InventoryTurnover.DayPeriod AS DayPeriod,
	InventoryTurnover.WeekPeriod AS WeekPeriod,
	InventoryTurnover.TenDaysPeriod AS TenDaysPeriod,
	InventoryTurnover.MonthPeriod AS MonthPeriod,
	InventoryTurnover.QuarterPeriod AS QuarterPeriod,
	InventoryTurnover.YearPeriod AS YearPeriod,
	InventoryTurnover.Company AS Company,
	InventoryTurnover.StructuralUnit AS StructuralUnit,
	InventoryTurnover.GLAccount AS GLAccount,
	InventoryTurnover.Products AS Products,
	InventoryTurnover.Characteristic AS Characteristic,
	InventoryTurnover.Batch AS Batch,
	InventoryTurnover.Products.MeasurementUnit AS MeasurementUnit,
	CASE
		WHEN VALUETYPE(InventoryTurnover.Recorder) = TYPE(Document.InventoryIncrease)
			THEN InventoryTurnover.QuantityReceipt
		ELSE -InventoryTurnover.QuantityExpense
	END AS Quantity,
	CASE
		WHEN VALUETYPE(InventoryTurnover.Recorder) = TYPE(Document.InventoryIncrease)
			THEN InventoryTurnover.AmountReceipt
		ELSE -InventoryTurnover.AmountExpense
	END AS Amount,
	CASE
		WHEN VALUETYPE(InventoryTurnover.Recorder) = TYPE(Document.InventoryIncrease)
			THEN &amp;Excesses
		ELSE &amp;Shortage
	END AS RegradingType,
	CASE
		WHEN VALUETYPE(InventoryTurnover.Recorder) = TYPE(Document.InventoryIncrease)
				OR VALUETYPE(InventoryTurnover.Recorder) = TYPE(Document.InventoryWriteOff)
			THEN InventoryTurnover.Recorder.BasisDocument
		ELSE UNDEFINED
	END AS InventoryDocument
FROM
	AccumulationRegister.Inventory.Turnovers(, , Auto, ) AS InventoryTurnover
WHERE
	(VALUETYPE(InventoryTurnover.Recorder) = TYPE(Document.InventoryIncrease)
			OR VALUETYPE(InventoryTurnover.Recorder) = TYPE(Document.InventoryWriteOff))</query>
