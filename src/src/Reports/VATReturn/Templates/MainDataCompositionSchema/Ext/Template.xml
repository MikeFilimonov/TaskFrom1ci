SELECT
	TT_InputOutputRawDetails.Register AS Register,
	TT_InputOutputRawDetails.Period AS Period,
	TT_InputOutputRawDetails.Recorder AS Recorder,
	TT_InputOutputRawDetails.Company AS Company,
	TT_InputOutputRawDetails.Counterparty AS Counterparty,
	TT_InputOutputRawDetails.ShipmentDocument AS ShipmentDocument,
	TT_InputOutputRawDetails.VATRate AS VATRate,
	TT_InputOutputRawDetails.OperationType AS OperationType,
	TT_InputOutputRawDetails.ProductType AS ProductType,
	TT_InputOutputRawDetails.AmountExcludesVAT AS AmountExcludesVAT,
	TT_InputOutputRawDetails.VATAmount AS VATAmount,
	TT_OperationTypesAndBoxes.Box AS Box
INTO TT_InputOutputDetails
FROM
	TT_InputOutputRawDetails AS TT_InputOutputRawDetails
		LEFT JOIN TT_OperationTypesAndBoxes AS TT_OperationTypesAndBoxes
		ON TT_InputOutputRawDetails.OperationType = TT_OperationTypesAndBoxes.OperationType
			AND TT_InputOutputRawDetails.Register = TT_OperationTypesAndBoxes.Register
;

