	TT_InputOutputDetails.Company AS Company,
	TT_InputOutputDetails.Counterparty AS Counterparty,
	TT_InputOutputDetails.ShipmentDocument AS ShipmentDocument,
	TT_InputOutputDetails.VATRate AS VATRate,
	TT_InputOutputDetails.OperationType AS OperationType,
	TT_InputOutputDetails.ProductType AS ProductType,
	TT_InputOutputDetails.AmountExcludesVAT AS AmountExcludesVAT,
	TT_InputOutputDetails.VATAmount AS VATAmount,
	TT_InputOutputDetails.Box AS Box,
	ISNULL(TT_SectionsAndBoxes.VATSection, "&lt;Undefined&gt;") AS VATSection
FROM
	TT_InputOutputDetails AS TT_InputOutputDetails
		LEFT JOIN TT_SectionsAndBoxes AS TT_SectionsAndBoxes
		ON TT_InputOutputDetails.Box = TT_SectionsAndBoxes.Box

UNION ALL

SELECT
	NULL,
	NULL,
	TT_InputOutputBoxTotals.Company,
	NULL,
	NULL,
	NULL,
	NULL,
	NULL,
	SUM(TT_InputOutputBoxTotals.AmountExcludesVAT),
	SUM(TT_InputOutputBoxTotals.VATAmount),
	TT_InputOutputBoxTotals.Box,
	ISNULL(TT_SectionsAndBoxes.VATSection, "&lt;Undefined&gt;")
FROM
	TT_InputOutputBoxTotals AS TT_InputOutputBoxTotals
		LEFT JOIN TT_SectionsAndBoxes AS TT_SectionsAndBoxes
		ON TT_InputOutputBoxTotals.Box = TT_SectionsAndBoxes.Box

GROUP BY
	ISNULL(TT_SectionsAndBoxes.VATSection, "&lt;Undefined&gt;"),
	TT_InputOutputBoxTotals.Company,
	TT_InputOutputBoxTotals.Box

UNION ALL

