		<query>SELECT
	"OUTPUT" AS Register,
	VATOutput.Period AS Period,
	VATOutput.Recorder AS Recorder,
	VATOutput.Company AS Company,
	VATOutput.Customer AS Counterparty,
	VATOutput.ShipmentDocument AS ShipmentDocument,
	VATOutput.VATRate AS VATRate,
	VATOutput.OperationType AS OperationType,
	VATOutput.ProductType AS ProductType,
	VATOutput.AmountExcludesVAT AS AmountExcludesVAT,
	VATOutput.VATAmount AS VATAmount
INTO TT_InputOutputRawDetails
FROM
	AccumulationRegister.VATOutput AS VATOutput
{WHERE
	(VATOutput.Period BETWEEN &amp;Date1 AND &amp;Date2)}

UNION ALL

SELECT
	"INPUT",
	VATInput.Period,
	VATInput.Recorder,
	VATInput.Company,
	VATInput.Supplier,
	VATInput.ShipmentDocument,
	VATInput.VATRate,
	VATInput.OperationType,
	VATInput.ProductType,
	VATInput.AmountExcludesVAT,
	VATInput.VATAmount
FROM
	AccumulationRegister.VATInput AS VATInput
{WHERE
	(VATInput.Period BETWEEN &amp;Date1 AND &amp;Date2)}
;

